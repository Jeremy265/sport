{
    "compilerOptions": {
        "jsx": "react-jsx",
        "module": "commonjs",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "outDir": "./dist/",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "es5"
    },
    "include": ["src"],
    "exclude": ["node_modules", "**/*.spec.ts", "**/*.test.ts"]
}